<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/cron/syncJiaoniCustomers</url>
        <description>Sync JiaoniDaiGou customers</description>
        <schedule>every day 04:00</schedule>
        <target>$VERSION</target>
        <timezone>America/Los_Angeles</timezone>
    </cron>
    <cron>
        <url>/cron/notification/xiaoxiong/orderStatus</url>
        <description>XiaoXiong daily updates</description>
        <schedule>every day 21:00</schedule>
        <timezone>Asia/Shanghai</timezone>
    </cron>
    <cron>
        <url>/cron/dumpJiaoniShippingOrder?limit=10</url>
        <description>Teddy full shipping orders dump</description>
        <schedule>every day 04:00</schedule>
        <target>$VERSION</target>
        <timezone>America/Los_Angeles</timezone>
    </cron>
</cronentries>
